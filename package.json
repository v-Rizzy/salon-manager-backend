{
  "name": "salon-manager-backend",
  "version": "0.0.0",
  "scripts": {
    "build": "node --env-file=./env/development.env -r ts-node/register ./scripts/build.ts",
    "dev": "node --env-file=./env/development.env -r ts-node/register ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src -e ts",
    "lint": "npx eslint ./src",
    "lint:tests": "npx eslint ./spec",
    "start": "node --env-file=./env/production.env -r ./preload.js ./dist",
    "test": "node --env-file=./env/test.env -r ts-node/register ./spec",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec -e ts"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.0.9",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin-ts": "^2.10.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.14.0",
    "eslint-plugin-n": "^17.13.1",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.4.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
}
